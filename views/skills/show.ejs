<%- include('../partials/header') %>
  <h1>Skill #<%= id + 1 %> Details</h1>
  <a href="/skills/<%= id %>/edit">Edit Skill</a>
  <br>
  <h3>Skill: <%= skill.skill %></h3>
  <h3>Level: <%= skill.level %></h3>
  <h3>Experience: <%= skill.experience %></h3>
  <br>
  <form action="/skills/<%= id %>?_method=DELETE" method="POST">
    <button type="submit">Delete Skill</button>
  </form>
<%- include('../partials/footer') %>